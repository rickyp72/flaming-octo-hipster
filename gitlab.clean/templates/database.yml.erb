production:
  adapter: mysql2
  encoding: utf8
  reconnect: false
  database: <%= @gitlab_dbname %>
  pool: 10
  username: <%= @gitlab_dbuser %>
  password: <%= @gitlab_dbpwd %>
  host: <%= @gitlab_dbhost %>
